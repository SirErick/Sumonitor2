<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.sumonitor2">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Sumonitor"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RegistroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DeviceList" />
        <activity android:name=".ledControl" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".MonitorActivity"></activity>
        <activity android:name=".MuestraActivity"></activity>
    </application>

</manifest>